        "id": "z",
        "legend": "Center",
        "name": "Center",
        "seats": Math.round(Q.z_r_thuringia * 0.53),
    });
}

if (Q.dvp_r_thuringia && !Q.lvp_formed) {
    data.push({
        "id": "dvp",
        "legend": "DVP",
        "name": "DVP",
        "seats": Math.round(Q.dvp_r_thuringia * 0.53),
    });
}

if (Q.other_r_thuringia) {
    let total = Q.farm_r_thuringia + Q.wp_r_thuringia; 
    if (total > 0) {
        let farmShare = (Q.farm_r_thuringia / total) * 0.56;
        let wpShare = (Q.wp_r_thuringia / total) * 0.56;
        data.push({
            "id": "farm3",
            "legend": "Farm",
            "name": "Farm",
            "seats": Math.round(Q.farm_r_thuringia * farmShare),
