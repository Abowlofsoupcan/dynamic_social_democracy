on-arrival: {!
// Einigkeit - survive without a civil war.
if (Q.verify_token && Q.verify_token()) {
    this.achieve('einigkeit');
}

// Br√ºder, zur Sonne, zur Freiheit - reduce unemployment, pause reparations, increase women's rights, elect Otto Braun president, and deport Hitler in one playthrough on at least normal difficulty.
if (Q.verify_token && Q.verify_token() && (Q.president == "Braun" || Q.president == "Schumacher" || Q.president == "Wels" || Q.president == "Grzesinski" || Q.president == "Juchacz" || Q.president == "Rosenfeld") && !(Q.chancellor_party == "NSDAP") && Q.unemployed < 20 && Q.year >= 1933 && !Q.civil_war_seen && Q.hitler_deported && Q.reparations <= -2 && Q.womens_rights >= 3) {
    this.achieve('bruder_zur_sonne');
}
// Freie Marktwirtschaft - win without doing an economic plan.
if (!Q.economic_plan && (Q.verify_token && Q.verify_token())) {
    this.achieve('freie_marktwirtschaft');
}
!}  

- @game_over: Game over.