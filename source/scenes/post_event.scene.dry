            Q.old_middle_dvp -= 5 /12;
        }
        Q.new_middle_ddp -= 5/12;
        Q.old_middle_ddp -= 5/12;
        Q.new_middle_nsdap += 8/12;
        Q.old_middle_nsdap += 8/12;
        Q.unemployed_nsdap += 6/12;
