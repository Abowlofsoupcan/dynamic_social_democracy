Q['pro_republic_' + Q.month] = Q.pro_republic
Q['dvp_relation_' + Q.month] = Q.dvp_relation
Q['dvp_ideology_' + Q.month] = Q.dvp_ideology
if (Q.dnf_decay_timer > 0) {
